{
  "name": "parcel-starter",
  "version": "1.0.0",
  "description": "Frontend project starter with Parcel",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "clear": "rmdir /s/q dist",
    "clear:cache": "rmdir /s/q .cache",
    "start": "parcel src/index.pug --open",
    "build": "parcel build src/index.pug --public-url .",
    "build:clear": "npm run clear && npm run clear:cache && npm run build"
  },
  "keywords": [
    "parcel",
    "starter",
    "template",
    "bundler"
  ],
  "author": "FurryCat",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-export-namespace-from": "^7.8.3",
    "@babel/plugin-proposal-function-bind": "^7.8.3",
    "@babel/plugin-proposal-numeric-separator": "^7.8.3",
    "@babel/plugin-syntax-export-default-from": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@babel/preset-env": "^7.8.4",
    "autoprefixer": "^9.7.4",
    "parcel-bundler": "^1.12.4",
    "postcss-modules": "^1.5.0",
    "pug": "^3.0.1",
    "sass": "^1.25.0"
  }
}
